import java.util.Scanner;
class Main {
    public static void main(String[] args) {
    Scanner scan=new Scanner(System.in);
    System.out.println("enter the integer value");
        int a=scan.nextInt();
        String binary="";
        while(a>0){
            binary=(a%2)+binary;
            a=a/2;
        }
        System.out.println("binary:"+binary);
    }  
}

        
