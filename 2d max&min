public class MaxElement2D{
    public static void main(String[]args){
        int[][] matrix={
            {12,34,54},
            {2,23,21},
            {6,43,53},
        };
        int max=0;
        for(int[] row : matrix){
            for(int num : row){
                if(num > max)
                max= num;
            }}
            System.out.println("Largest element:"+max);
            }
}
